{
	"name": "serverflow",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"bootstrap": "5.1.3",
		"react": "^17.0.2",
		"react-bootstrap": "^2.2.1",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.2",
		"react-scripts": "5.0.0",
		"web-vitals": "^2.1.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:ci": "jest --ci",
		"coverage:ci": "jest --ci --coverage --coverageDirectory='coverage'",
		"eject": "react-scripts eject",
		"eslint-prettier-check": "eslint-config-prettier ./src/App.js",
		"lint-check": "eslint . --ext .js,.tsx,.ts,.json",
		"lint": "eslint --fix . --ext .js,.tsx,.ts,.json"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.2.1",
		"@types/jest": "^27.0.1",
		"@types/node": "^16.7.13",
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest": "^26.1.1",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-jsx": "^0.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.3",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"lint-staged": "^12.3.5",
		"prettier": "^2.5.1",
		"semantic-release": "^19.0.2",
		"ts-jest": "^27.1.3",
		"typescript": "^4.4.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx,css,md,json}": [
			"prettier --write"
		],
		"*.{js,ts,tsx,json}": [
			"eslint --ext [.json,.tsx,.js,.ts] --fix"
		]
	}
}
